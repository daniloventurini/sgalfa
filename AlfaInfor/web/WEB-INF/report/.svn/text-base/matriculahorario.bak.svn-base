	<queryString><![CDATA[SELECT
     pessoa.`nome` AS pessoa_nome,
     matricula.`dataMatricula` AS matricula_dataMatricula,
     matricula.`dia` AS matricula_dia,
     matricula.`horario` AS matricula_horario,
     matricula.`id` AS matricula_id
FROM
     `aluno` aluno INNER JOIN `matricula` matricula ON aluno.`id` = matricula.`aluno_id`
     INNER JOIN `pessoafisica` pessoafisica ON aluno.`id` = pessoafisica.`id`
     INNER JOIN `pessoa` pessoa ON pessoafisica.`id` = pessoa.`id`
WHERE
     matricula.`dia` LIKE $P{pDia} AND matricula.`horario` LIKE $P{pHora}]]></queryString>

